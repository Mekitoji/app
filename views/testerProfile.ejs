<!DOCTYPE html>
<html lang="en" ng-app="project">
<head>
  <meta charset="UTF-8">
  <title><%=tester.name%></title>
  <script src= "../../../vendor/jquery/dist/jquery.min.js"></script>
  <script src= "../../../vendor/jquery/jquery-ui-1.11.1/jquery-ui.min.js"></script>
  <link rel="stylesheet" href="../../../vendor/jquery/jquery-ui-1.11.1/jquery-ui.min.css">
  <link rel="stylesheet" href="../../../vendor/bootstrap/dist/css/bootstrap.min.css">
  <script src= "../../../vendor/bootstrap/dist/js/bootstrap.min.js"></script>

  <script src= "../../../vendor/angular/angular.min.js"></script>

  <script src= "../../../vendor/underscore/underscore.min.js"></script>

  <!-- angular -->
  <script src= '../../../js/new-core.js'></script>
  <script src= "../../../vendor/angular-resource/angular-resource.min.js"></script>
  <script src= "../../../vendor/angular-route/angular-route.min.js"></script>
  <script src= "../../../vendor/angular-bootstrap/ui-bootstrap.min.js"></script>
  <script src= "../../../vendor/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>
  <script src= '../../../vendor/ng-grid-2.0.14/ng-grid-2.0.14.min.js'></script>
  <script src= '../../../vendor/ui-bootstrap-custom-tpls-0.10.0.min.js'></script>

  <script src= '../../../js/controllers/kpiProfile.js'></script>

  <% var route = path.split('/')[1]%>
  <% if(route === 'cis') { %>
    <script src='../../../js/factories/CIS/Apps.js'></script>
    <script src='../../../js/factories/CIS/Testers.js'></script>
    <script src='../../../js/factories/CIS/Calendar.js'></script>
    <script src='../../../js/factories/CIS/Tester.js'></script>
  <% } else if(route === 'eu') {%>
    <script src='../../../js/factories/EU/Apps.js'></script>
    <script src='../../../js/factories/EU/Testers.js'></script>
    <script src='../../../js/factories/EU/Calendar.js'></script>
    <script src='../../../js/factories/EU/Tester.js'></script>
  <% } else if(route === 'global') { %>
    <script src='../../../js/factories/Sandbox/Apps.js'></script>
    <script src='../../../js/factories/Sandbox/Testers.js'></script>
    <script src='../../../js/factories/Sandbox/Calendar.js'></script>
    <script src='../../../js/factories/Sandbox/Tester.js'></script>
  <% } else if(route === 'sia') { %>
    <script src='../../../js/factories/SIA/Apps.js'></script>
    <script src='../../../js/factories/SIA/Testers.js'></script>
    <script src='../../../js/factories/SIA/Calendar.js'></script>
    <script src='../../../js/factories/SIA/Tester.js'></script>
  <% } %>

  <script>
  $(function () {
    $('[data-toggle="popover"]').popover({"html":true, trigger:"hover"})
  });
  </script>
  <style>
    tr>th {
      border: 0px !important;
    }
    .green {
      background-color: green !important;
    }
  </style>

<style>
  h2.title {
    text-align: center;
  }

  #ppdatepicker {
    visibility:hidden;
    width:1px;
    position:absolute;
    top:5px;
  }
</style>

<script>
  $(document).ready(function () {
    $("#ppdatepicker").datepicker({
      onSelect: function(dateText, inst){
        var date = new Date(dateText);
        var day = date.getDate();
        var month = date.getMonth()+1;
        var year = date.getFullYear();
        var xx = month+ "-"+ day + "-" +year;
        var currentLoc = window.location.pathname;
        var tempArray= currentLoc.split("/");
        tempArray[2] = "history";
        tempArray.splice(3, 0, xx);
        tempArray.splice(5, 1);
        window.location.pathname = tempArray.join("/");
      }
    });
    $('#pdatepicker').click(function(){
      $('#ppdatepicker').datepicker("show");
    });
  });
</script>
</head>
<body ng-controller="kpi-profile">
    <% include layout/header%>
    <div class="container">
    <% if(tester.user===null) {%>
      <%tester.user={};%>
    <%}%>

  <button type="button" class="btn btn-lg" data-toggle="popover" title="<%= tester.user.local.username.first %> <%= tester.user.local.username.last %>" data-content="<b>TesterId:</b><%=tester._id%><br> <b>User Id:</b>  <%=tester.user._id%><br> <b>Mail:</b>  <%=tester.user.local.email   %> <br> <b>Group:</b>  <%=tester.user.local.group%>"> <%=tester.name%></button>
  <select class ="year-filter"  ng-model="filter.year">
    <option value="all">all</option>
    <option value="2015">2015</option>
    <option value="2016">2016</option>
    <option value="2017">2017</option>
    <option value="2018">2018</option>
  </select>
  <table class="table table-hover">
    <tr>
      <th>Application name</th>
      <th>Review Time</th>
      <th>Test Cycles</th>
      <th>Info about app</th>
    </tr>
    <tr>

    </tr>
  </table>

  <table class="table-hover table">
    <tr>
      <th>Application name</th>
      <th>Review Time</th>
      <th>Test Cycles</th>
      <th>Info about app</th>
    </tr>
  <% var total={respTime:0,testCycles:0,countResp:0,countCycle:0} %>
  <% tester.appStorage = tester.appStorage.sort(function(a, b) {
    return a.app.appName.toLowerCase().localeCompare(b.app.appName.toLowerCase());
  });%>
  <% for(var i =0;i<tester.appStorage.length;i++) { %>
  <tr>
  <%  if(tester.appStorage[i].year == year) {
  var color=""; %>

    <% if(tester.appStorage[i].app===null) {
      tester.appStorage[i].app={};
    }
    if(tester.appStorage[i].respTime !== 0) {
      total.countResp++;
      total.respTime+=tester.appStorage[i].respTime
    } else {
      tester.appStorage[i].respTime = "N/A";
    } %>

    <% if(tester.appStorage[i].app.tv==='Approved' || tester.appStorage[i].app.tv==='Partial') { %>
      <% total.testCycles+=tester.appStorage[i].testCycle;
      total.countCycle++;
      color = "green" %>
      <td> <%= tester.appStorage[i].app.appName%></td>
    <% } else { %>
      <td> <%= tester.appStorage[i].app.appName%></td>
    <% } %>

    <% if(tester.appStorage[i].respTime!=='N/A') { %>
      <td> <%= tester.appStorage[i].respTime.toFixed(2)%></td>
    <% } else { %>
      <td> <%= tester.appStorage[i].respTime%></td>
    <% } %>
    <td> <%= tester.appStorage[i].testCycle%></td>
    <td> <button type="button" class="btn <%= color%>" data-toggle="popover" title="<%= tester.appStorage[i].app.appName%>" data-content="<b>App Id:</b> <%=tester.appStorage[i].app.applicationId.split("/")[0]%> <br> <b>Seller:</b> <%=tester.appStorage[i].app.seller%> <br> <b>SDP status:</b> <%=tester.appStorage[i].app.sdpStatus%> <br> <b>TV:</b> <%=tester.appStorage[i].app.tv%> <br> <b>Current status:</b> <%=tester.appStorage[i].app.currentStatus%> <br> <b>Update Date:</b> <%=tester.appStorage[i].app.updateTime.toLocaleDateString()%> <br> <b>Review Time: </b><%=tester.appStorage[i].app.replyTime.toFixed(2) %> <br> <b>Test Cycles:</b> <%=tester.appStorage[i].app.testCycles%> <br> <b>Last Resp:</b> <%=tester.appStorage[i].app.resp%> <br><b> Outdated Status: </b><%=tester.appStorage[i].app.outdated%> <br> ">More Info</button> </td>
  </tr>

    <% }} %>
    <%total.respTime=total.respTime/total.countResp%>
    <%total.testCycles=total.testCycles/total.countCycle%>
  <tr>
    <td><b>Total:</b></td>
    <% if(!isNaN(total.respTime)) { %>
      <td><b><%=total.respTime.toFixed(2) %></b></td>
    <% } else { %>
      <td><b>N/A</b></td>
    <% } %>
    <% if(!isNaN(total.testCycles)) { %>
      <td><b><%=total.testCycles.toFixed(2) %></b></td>
    <% } else { %>
      <td><b>N/A</b></td>
    <% } %>
    <td></td>
  </tr>
  </table>
  </div>
</body>
</html>