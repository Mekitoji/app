<!doctype html >
<html ng-app='project'>
<head>
  <title>GKSA</title>
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
  <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>

  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.0-beta.13/angular.min.js"></script>

  <script src="js/core.js"></script> <!-- load our main application -->
</head>

<body ng-controller='mainController'>

  <% include layout/head %>

  <!-- LIST NAVBAR -->
  <ul class="nav nav-tabs">
    <li class="active"><a href="#list" data-toggle="tab">List of apps</a></li>
    <li><a href="#calendar" data-toggle="tab">Calendar</a></li>
    <li><a href="#test-cycles" data-toggle="tab">Test Cycles</a></li>
  </ul>
  
  <div class="tab-content" >





  <div class="tab-pane fade in active " id="list">




  <div class="container">

<div id="app-list" class="row">
    <table id='main-list-table' class=" table table-striped table-bordered table-hover table-responsive">
      <thead>
        <tr>
          <th>No</th>
          <th>Country</th>
          <th>App name</th>
          <th>Category</th>
          <th>SDP Status</th>
          <th>Update Date</th>  
          <th>Seller</th>
          <th>TV</th>
          <th>Current status</th>
          <th>Test Cycles</th>
          <th>Reply Time</th>
          <th>Resp</th>
        </tr>
      </thead>
      <tr ng-repeat='app in apps | filter:search'>
        <td>{{app.no}}</td>
        <td>{{app.country}}</td>
        <td>{{app.appName}}</td>
        <td>{{app.category}}</td>
        <td>{{app.sdpStatus}}</td>
        <td>{{app.updateTime}}</td>
        <td>{{app.seller}}</td>
        <td>{{app.tv}}</td>
        <td>{{app.currentStatus}}</td>
        <td>{{app.testCycles}}</td>
        <td>{{app.replyTime}}</td>
        <td>{{app.resp}}</td>

      </tr>
    </table>
  </div>


    <!-- FORM TO CREATE TODOS -->

        <form>
          <div class="form-group">

            <!-- BIND THIS VALUE TO formData.text IN ANGULAR -->
            <label>Country</label>
            <select name="country" id="table-country" ng-model="formData.country">
              <option value="Russia">Russia</option>
              <option value="Ukraine">Ukraine</option>
              <option value="Belarus">Belarus</option>
              <option value="Latvia">Latvia</option>
              <option value="Kazakhstan">Kazakhstan</option>
              <option value="Lithuania">Lithuania</option>
              <option value="Estonia">Estonia</option>
              <option value="Uzbekistan">Uzbekistan</option>
            </select> <br>
            <label>Application name</label>
            <input type="text" placeholder="say" ng-model="formData.appName"> <br>
            <label> Category</label>
            <select name="category" id="table-category" ng-model='formData.category'>
              <option value="OTT">OTT</option>
              <option value="Pay TV">Pay TV</option>
              <option value="Broadcast">Broadcast</option>
              <option value="OTT + Pay TV">OTT + Pay TV</option>
            <option value="Others">Others</option>
            </select> <br>
            <label>SDP status</label>
            <select name="sdp-status" id="table-sdp-status" ng-model='formData.sdpStatus'>
              <option value="Gk review request">Gk review request</option>
              <option value="GK rewiev">GK rewiev</option>
              <option value="GK Review Reject">GK Review Reject</option>
              <option value="Verification Request">Verification Request</option>
              <option value="Pre-test">Pre-test</option>
              <option value="Function Testing">Function Testing</option>
              <option value="Content Testing">Content Testing</option>
              <option value="Final review">Final review</option>
              <option value="App QA Approved">App QA Approved</option>
              <option value="App QA Rejected">App QA Rejected</option>
            </select> <br>
            <label>Update Time</label>
            <input type="date" placeholder="say" ng-model="formData.updateTime"> <br>
            <label>Seller</label>
            <input type="text" placeholder="say" ng-model="formData.seller"> <br>
            <label>Tv</label>
            <select name="Tv" id="table-tv" ng-model='formData.tv'>
              <option value="Reject">Reject</option>
              <option value="Approve">Approve</option>
              <option value="Partial">Partial</option>
            </select> <br>
            <label>Current status</label>
            <select name="current-status" id="table-current-status" ng-model='formData.currentStatus'>
              <option value="Waiting for fix">Waiting for fix</option>
              <option value="Waiting for test">Waiting for test</option>
              <option value="Waiting for QA">Waiting for QA</option>
            </select> <br>
            <label>Test Cycles</label>
            <input type="text" placeholder="say" ng-model="formData.testCycles"> <br>
            <label>Reply Time</label>
            <input type="text" placeholder="say" ng-model="formData.replyTime"> <br>
            <label>Resp</label>
            <select name="resp" id="table-resp" ng-model='formData.resp'>
              <option value="YK">YK</option>
              <option value="DP">DP</option>
              <option value="VE">VE</option>
              <option value="AS">AS</option>
            </select> <br>
          </div>

          <!-- createToDo() WILL CREATE NEW TODOS -->
          <button type="submit" class="btn btn-primary btn-lg" ng-click="createApp()">Add</button>
        </form>




  </div>
    </div>

    <div class="tab-pane fade" id="calendar">
      
    </div>
    <div class="tab-pane fade" id="test-cycles">
      
    </div>
  </div>

</body>
</html>