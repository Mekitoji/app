    <tr>
      <td class='take-app-name'>{{name}}</td>
      <td><select name="" id=""></select></td>
      <td><select name="" id=""></select></td>
      <td><select name="" id=""></select></td>
      <td><select name="" id=""></select></td>
      <td><select name="" id=""></select></td>
      <td><select name="" id=""></select></td>
      <td><select name="" id=""></select></td>
